package com.thread;

class Th1 extends Thread{
	int sum;
	
	@Override
	public void run() {
		for(int i=1;i<=20;i++) {
			sum += i;
		}
	}
	public int getSum() {
		return sum;
	}
	
}



class Th2 extends Thread{
	int sum;
	
	@Override
	public void run() {
		for(int i=1;i<=40;i++) {
			sum += i;
		}
	}
	public int getSum() {
		return sum;
	}
}

