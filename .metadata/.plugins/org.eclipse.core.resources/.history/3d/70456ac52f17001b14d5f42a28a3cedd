package com.thread;

class T extends Thread{

	@Override
	public void run() {
		while(true) {
			System.out.println("Thread ....");
			try {
				Thread.sleep(500);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
	
	
}


public class Test4 {

	public static void main(String[] args) throws InterruptedException {
		T t = new T();
		t.start();
		Thread.sleep(10000);
		System.out.println("End Application....");
	}

}
